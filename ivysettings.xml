<ivysettings>
        
            <settings defaultResolver="chain"/>
    <resolvers>
        <chain name="chain">
            <ibiblio name="central" m2compatible="true"/>
            <ibiblio name="Maven FreeHEP" m2compatible="true" root="http://java.freehep.org/maven2"/>
